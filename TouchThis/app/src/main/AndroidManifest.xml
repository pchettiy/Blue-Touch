<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.praba1110.touchthis" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_action"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Game_activity"
            android:label="@string/title_activity_game_activity"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".GameOver"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_game_over" >
        </activity>
        <activity
            android:name=".highscores"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_highscores" >
        </activity>
        <activity
            android:name=".Difficulty"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_difficulty" >
        </activity>
    </application>

</manifest>
